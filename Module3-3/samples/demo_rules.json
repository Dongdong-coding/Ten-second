{
  "ruleset": {
    "id": "lease-baseline",
    "version": "1.0.0",
    "description": "Baseline lease checks",
    "engine_semver": {"min": "1.0.0", "max": "1.1.0"}
  },
  "rules": [
    {
      "rule_id": "MONEY_DEPOSIT_MINIMUM",
      "version": "1.0.0",
      "scope": {
        "category": "MONEY.DEPOSIT",
        "canonical_terms": ["DEPOSIT_AMOUNT"]
      },
      "matchers": [
        {"type": "keyword", "pattern": "minimum deposit"},
        {"type": "regex", "pattern": "deposit of \\$(?P<amount>\\d+)"}
      ],
      "severity": "HIGH",
      "weight": 0.8,
      "priority": 10,
      "evidence_hints": ["deposit clause"],
      "requires": ["numeric_amount"],
      "flags": ["baseline"],
      "activation": {"status": "active"}
    },
    {
      "rule_id": "NOTICE_PERIOD_DISCLOSURE",
      "version": "1.0.0",
      "scope": {
        "category": "TIME.NOTICE",
        "canonical_terms": ["NOTICE_PERIOD"]
      },
      "matchers": [
        {"type": "phrase", "pattern": "tenant must provide"}
      ],
      "severity": "WARN",
      "weight": 1.0,
      "priority": 20,
      "evidence_hints": ["notice clause"],
      "requires": ["date_range"],
      "flags": ["baseline"],
      "activation": {"status": "active"}
    }
  ],
  "experiment": {
    "variants": {"baseline": 70.0, "canary": 30.0},
    "sticky_scope": "document_id"
  }
}
